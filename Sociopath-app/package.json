{
  "version": "1.0.0",
  "name": "ASP_NET",
  "private": true,
  "devDependencies": {
    "browserify": "9.0.3",
    "browserify-shim": "3.8.3",
    "gulp": "^3.9.0",
    "gulp-sourcemaps": "^1.6.0",
    "jquery": "1.8.2",
    "jquery-mobile": "1.4.1",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
  },
  "browser": {
    "jquery": "./www/js/libs/jquery-1.8.2.min.js",
    "jquery-mobile": "./www/js/libs/jquery.mobile-1.4.5.min.js",
    "knockout": "./www/js/libs/knockout-3.1.0.js"
  },
  "browserify-shim": {
    "jquery": "$",
    "jquery-mobile": {
      "exports": "$.mobile",
      "depends": [
        "jquery:$"
      ]
    },
    "knockout": {
      "exports": "ko",
      "depends": [
        "jquery:$"
      ]
    }
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  }
}
